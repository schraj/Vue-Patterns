{
    "schema": {
        "element": "lni-form",
        "attributes": {
            "inputName": "test-form"
        },
        "children": [
            {
                "element": "lni-fieldset",
                "attributes": {
                    "label": "test legend"
                },
                "children": [
                    {
                        "element": "lni-name-input",
                        "attributes": {
                            "model": "userName",
                            "readonly": true,
                            "disabled": true,
                            "values": [
                                {"propertyName": "userName", "propertyValue": ""}
                            ]
                        }
                    },
                    {
                        "element": "lni-checkbox",
                        "attributes": {
                            "label": "Company Name is visible",
                            "model": "companyNameIsVisible",
                            "default": true,
                            "values": [
                                {"propertyName": "companyNameIsVisible", "propertyValue": ""}
                            ]
                        }
                    },
                    {
                        "element": "lni-textbox",
                        "attributes": {
                            "inputType": "text",
                            "label": "Company Name",
                            "model": "companyName",
                            "readonly": false,
                            "required": true,
                            "disabled": false,
                            "placeholder": "Company name",
                            "validator": "VueFormGenerator.validators.string",
                            "dependsOnValue": "{\"companyNameIsVisible\":true}",
                            "values": [
                                {"propertyName": "companyName", "propertyValue": ""}
                            ]
                        }
                    },
                    {
                        "element": "lni-checkbox",
                        "attributes": {
                            "label": "Boss Name is visible",
                            "model": "bossNameIsVisible",
                            "default": true
                        }
                    },
                    {
                        "element": "lni-textbox",
                        "attributes": {
                            "inputType": "text",
                            "label": "Boss Name",
                            "model": "bossName",
                            "readonly": false,
                            "required": true,
                            "disabled": false,
                            "placeholder": "Boss name",
                            "validator": "VueFormGenerator.validators.string",
                            "dependsOnFunction": "bossNameIsVisible"
                        }
                    },
                    {
                        "element": "lni-textbox",
                        "attributes": {
                            "inputType": "password",
                            "label": "Password",
                            "model": "password",
                            "min": 6,
                            "required": true,
                            "hint": "Minimum 6 characters",
                            "validator": "VueFormGenerator.validators.string"
                        }
                    },
                    {
                        "element": "lni-textbox",
                        "attributes": {
                            "inputType": "number",
                            "label": "Age",
                            "model": "age",
                            "min": 18,
                            "validator": "VueFormGenerator.validators.number"
                        }
                    },
                    {
                        "element": "lni-textbox",
                        "attributes": {
                            "inputType": "email",
                            "label": "E-mail",
                            "model": "email",
                            "placeholder": "User's e-mail address",
                            "validator": "VueFormGenerator.validators.email"
                        }
                    },
                    {
                        "element": "lni-checklist",
                        "attributes": {
                            "label": "Skills",
                            "model": "skills",
                            "multi": true,
                            "required": true,
                            "multiSelect": true,
                            "values": [
                                "HTML5",
                                "Javascript",
                                "CSS3",
                                "CoffeeScript",
                                "AngularJS",
                                "ReactJS",
                                "VueJS"
                            ]
                        }
                    },
                    {
                        "element": "lni-submit",
                        "attributes": {
                            "inputName": "test input Name",
                            "buttonText": "Submit"
                        }
                    }
                ]
            }
        ]
    }
}